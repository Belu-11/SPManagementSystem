@model IEnumerable<Product>

@if(Model != null && Model.Count() > 0)
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Product Name</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var prod in Model)
            {
                <tr>
                    <th>@prod.Name</th>
                    <th>@prod.Quantity</th>
                </tr>
            }
        </tbody>
    </table>
}